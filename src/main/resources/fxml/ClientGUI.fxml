                                       <children>
                                          <TextField fx:id="textLWM2MIP" alignment="CENTER" prefWidth="200.0" promptText="LWM2M IP address">
                                             <HBox.margin>
                                                <Insets left="16.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <TextField fx:id="textLWM2MPort" alignment="CENTER" prefWidth="60.0" promptText="Port" />
