                                    </HBox>
                                    <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0">
                                       <children>
                                          <Button fx:id="buttonUpdateClient" mnemonicParsing="false" onAction="#handleButtonUpdateClient" text="Update &amp; Connect" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                  </content>
